  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "vue-tsc --noEmit",
    "b": "pnpm build && pnpm preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset --force && prisma db push",
    "update": "npx npm-check-updates -t latest --interactive",
    "zenstack:generate": "zenstack generate --schema ./.zenstack/schema.zmodel && bun ./scripts/on-prisma.js",
    "zenstack:generate-prod": "zenstack generate --schema /app/.zenstack/schema.zmodel && rm -rf /app/prisma && mv /app/.zenstack/prisma /app/prisma",
    "submodule:checkout-master": "git submodule foreach 'git checkout master && git pull'",
    "submodule:commit": "bun scripts/git-modules-commit.js",
    "submodule:push": "git submodule foreach 'git push'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:scripts": "bun scripts.js"
  },
